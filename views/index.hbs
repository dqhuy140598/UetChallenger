<head>
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<meta name="author" content="M_Adnan" />
<!-- Document Title -->
<title>UET - Challenger</title>

<!-- Favicon -->
<link rel="shortcut icon" href="../frontend/images/favicon.ico" type="image/x-icon">
<link rel="icon" href="../frontend/images/favicon.ico" type="image/x-icon">

<!-- SLIDER REVOLUTION 4.x CSS SETTINGS -->
<link rel="stylesheet" type="text/css" href="../frontend/rs-plugin/css/settings.css" media="screen" />

<!-- StyleSheets -->
<link rel="stylesheet" href="../frontend/css/ionicons.min.css">
<link rel="stylesheet" href="../frontend/css/bootstrap.min.css">
<link rel="stylesheet" href="../frontend/css/font-awesome.min.css">
<link rel="stylesheet" href="../frontend/css/main.css">
<link rel="stylesheet" href="../frontend/css/style.css">
<link rel="stylesheet" href="../frontend/css/responsive.css">

<style>
  #content{
    background:cadetblue;
  }
  #rank td{
    font-family: 'Courier New', Courier, monospace;
    font-size:20px;
  }
  #problems td a{
    font-family:'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
    font-size: 16px;
  }
  #problems th{
    font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
    color:cornflowerblue;
  }
  #myCarousel{
    overflow: hidden;
  }
</style>

<!-- Fonts Online -->
<link href="https://fonts.googleapis.com/css?family=Lato:100i,300,400,700,900" rel="stylesheet">

<!-- JavaScripts -->
<script src="../frontend/js/vendors/modernizr.js"></script>

</head>
<body>

<!-- Page Wrapper -->

  <div id="warp" class="layout-1 container" >
  {{>header user = user}}
  <div class="container" style="margin-top:2%;padding-left:0px">
          <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
              <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
              <li data-target="#myCarousel" data-slide-to="1"></li>
              <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner">

              <div class="item active">
                <img src="../frontend/images/ban3.jpg" alt="Los Angeles" style="width:100%;">
              </div>

              <div class="item">
                <img src="../frontend/images/ban1.jpg" alt="Chicago" style="width:100%;">
            
              </div>
            
              <div class="item">
                <img src="../frontend/images/ban2.png" alt="New York" style="width:100%;">
            
              </div>
          
            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#myCarousel" data-slide="prev">
              <span class="glyphicon glyphicon-chevron-left"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" data-slide="next">
              <span class="glyphicon glyphicon-chevron-right"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
      </div>
  <div id="content">
      <section class="under-items padding-top-60">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <div class="row"> 
              <div class="col-sm-12"> 
                  <h5 class="lead text-left">Danh Sách Bài Tập</h5>
                  <table id="problems" class="table table-hover">
                      <thead>
                        <tr>
                          <th class="lead">Tên Bài</th>
                          <th class="lead">Độ Khó</th>
                          <th class="lead">Tổng Số Lần Submit</th>
                        </tr>
                      </thead>
                      <tbody>
                        {{#each listproblem}}
                          <tr>
                            <td><a class="text-info" href="./problems/{{this._id}}">{{this.name}}</a></td>
                            <td><a class="text-success" href="./tag/{{this.tag}}">{{this.type}}</a></td>
                            <td class="text-danger">{{this.totalSubmit}}</td>
                          </tr> 
                        {{/each}}     
                      </tbody>
                    </table>
              </div>
            </div>
          </div>
          
          <!-- Product Slider -->
          <div class="col-md-4">
            <div class="product">
              <article class="slider-item on-nav"> 
                <h5 class="lead bg-success text-center">Bảng Xếp Hạng</h5>
                <table id="rank" class="table">
                  <thead>
                    <tr>
                      <th class="lead text-primary">Họ Và Tên</th>
                      <th class="lead text-danger">Điểm</th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#each wallofframe}}
                      <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.totalScore}}</td>
                      </tr>
                    {{/each}}      
                  </tbody>
                </table>
                <div class="thumb-slider">
                  <ul class="slides">
                    <li data-thumb="images/item-img-1-6.jpg"> <img src="../frontend/images/algo-og.jpg" alt=""> </li>
                    <li data-thumb="images/item-img-1-7.jpg"> <img src="../frontend/images/DSC_0102.jpg" alt=""> </li>
                    <li data-thumb="images/item-img-1-15.jpg"> <img src="../frontend/images/programming_01_0.jpg" alt=""> </li>
                  </ul>
                </div>
              </article>
            </div>
          </div>
        </div>
      </div>
    </section>
    <section class="padding-top-60 padding-bottom-60">
      <div class="container"> 
        <div class="heading">
            <h2>Từ Blog của chúng tôi</h2>
            <hr>
        </div>
        <div class="row">
          {{#each listthreeblogs}}
          <div class="col-md-4">
              <div class="blog-post">
                <article class="row">
                  <div class="col-xs-7"> <img class="img-responsive" src="{{this.image}}" alt="" width="60%" > </div>
                  <div class="col-xs-5"> <span><i class="fa fa-bookmark-o"></i> 25 Dec, 2017</span> <a href="./blogs/{{this._id}}" id="title" class="tittle" style="font-size:16px">{{this.title}} </a>
                    <a href="./blogs/{{this._id}}">Readmore</a></div>
                </article>
              </div>
            
          </div>
          {{/each}}
        </div>       
      </div>
    </section>
  </div>
  
  {{>footer}}
  </div>
<!-- End Page Wrapper --> 

<!-- JavaScripts --> 
<script src="../frontend/js/vendors/jquery/jquery.min.js"></script> 
<script src="../frontend/js/vendors/wow.min.js"></script> 
<script src="../frontend/js/vendors/bootstrap.min.js"></script> 
<script src="../frontend/js/vendors/own-menu.js"></script> 
<script src="../frontend/js/vendors/jquery.sticky.js"></script> 
<script src="../frontend/js/vendors/owl.carousel.min.js"></script> 

<!-- SLIDER REVOLUTION 4.x SCRIPTS  --> 
<script type="text/javascript" src="../frontend/rs-plugin/js/jquery.tp.t.min.js"></script> 
<script type="text/javascript" src="../frontend/rs-plugin/js/jquery.tp.min.js"></script> 
<script type="text/javascript" src="../frontend/js/main.js"></script> 
<script type="text/javascript" src="../frontend/js/vendors/jquery.nouislider.min.js"></script> 
<script>
jQuery(document).ready(function($) {
  
  //  Price Filter ( noUiSlider Plugin)
    $("#price-range").noUiSlider({
    range: {
      'min': [ 0 ],
      'max': [ 1000 ]
    },
    start: [40, 940],
        connect:true,
        serialization:{
            lower: [
        $.Link({
          target: $("#price-min")
        })
      ],
      upper: [
        $.Link({
          target: $("#price-max")
        })
      ],
      format: {
      // Set formatting
        decimals: 2,
        prefix: '$'
      }
        }
  })
})

</script>
</body>

