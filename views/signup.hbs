<body>

<!-- Page Wrapper -->

    <div class="top-bar">
      <div class="container">
        <p>Welcome to Uet Challenger!</p>
        <div class="right-sec">
          <ul>
            {{#if user}}
                  <span class="text text-primary">Chào: {{user.name}}</span>
                  <li><a href="/user/logout"><span class="text text-danger">Đăng Xuất</span></a></li>
            {{else}}
                  <li><a href="/user/login">Login</a></li>
                  <li><a href="/user/signup">Register</a></li>
            {{/if}}
          </ul>
        
        </div>
      </div>
    </div>


  <!-- Header -->
  <header>
    <div class="container">
      <div class="logo"> <a href="index-2.html"><img src="../images/logo.png" alt="" ></a> </div>
      
      <!-- Cart Part -->
    </div>
    
    <!-- Nav -->
    <nav class="navbar ownmenu">
      <div class="container"> 
        
        <!-- Categories -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-open-btn" aria-expanded="false"> <span><i class="fa fa-navicon"></i></span> </button>
        </div>
        
        <!-- NAV -->
        <div class="collapse navbar-collapse" id="nav-open-btn">
          <ul class="nav">
            <li class="megamenu active"> <a href="index-2.html" class="" data-toggle="dropdown">Home </a>
            </li>
            <li class=""> <a href="index-2.html" class="" data-toggle="dropdown">Pages </a>
            </li>
            <!-- Mega Menu Nav -->
            <li class="dropdown megamenu"> <a href="index-2.html" class="" data-toggle="dropdown">Mega menu </a>
            </li>
            <li class="dropdown"> <a href="blog.html" class="" data-toggle="dropdown">Blog</a>
            </li>
            <li> <a href="shop.html">Buy theme! </a></li>
          </ul>
        </div>
        
        <!-- NAV RIGHT -->
        <div class="nav-right"> </div>
      </div>
    </nav>
  </header>
  
  <!-- Content -->
  <div id="content"> 
    
    <!-- Linking -->
    <div class="linking">
      <div class="container">
        <ol class="breadcrumb">
          <li><a href="#">Home</a></li>
          <li class="active">Authentication</li>
        </ol>
      </div>
    </div>
    
    <!-- Blog -->
    <section class="login-sec padding-top-30 padding-bottom-100">
      <div class="container">
         {{#if user}}
            <div class="well bg-primary">
                <h5 class="text text-danger">{{err}}</h5>
            </div>
        {{else}}
          {{#if err}}
              <div class="well bg-primary">
                <h5 class="text text-danger">{{err}}</h5>
              </div>
          {{/if}}
        <div class="row">       
          <!-- Don’t have an Account? Register now -->
          <div class="col-md-6">
            <h5>Chưa Có Tài Khoản, Hãy Đăng Ký Ngay</h5>
            
            <!-- FORM -->
            <form method="POST" action="/user/signup">
              <ul class="row">
                <li class="col-sm-12">
                  <label>Họ Và Tên
                    <input type="text" class="form-control" name="name" placeholder="">
                  </label>
                </li>
                <li class="col-sm-12">
                  <label>Tên Đăng Nhập
                    <input type="text" class="form-control" name="username" placeholder="">
                  </label>
                </li>
                <li class="col-sm-12">
                  <label>Email
                    <input type="text" class="form-control" name="email" placeholder="">
                  </label>
                </li>
                <li class="col-sm-12">
                  <label>Mật Khẩu
                    <input type="password" class="form-control" name="password" placeholder="">
                  </label>
                </li>
                <li class="col-sm-12">
                  <label>Xác Nhận Mật Khẩu
                    <input type="password" class="form-control" name="passwordconf" placeholder="">
                  </label>
                </li>
                <li class="col-sm-12 text-left">
                  <button type="submit" class="btn-round">Register</button>
                </li>
              </ul>
            </form>
          </div>
        </div>
        {{/if}}
      </div>
    </section>
    
    <!-- Clients img -->
    <section class="light-gry-bg clients-img">
      <div class="container">
        <ul>
          <li><img src="./images/c-img-1.png" alt="" ></li>
          <li><img src="./images/c-img-2.png" alt="" ></li>
          <li><img src="./images/c-img-3.png" alt="" ></li>
          <li><img src="./images/c-img-4.png" alt="" ></li>
          <li><img src="./images/c-img-5.png" alt="" ></li>
        </ul>
      </div>
    </section>
    

  </div>
  <!-- End Content --> 
  
  <!-- Footer -->
  <footer>
    <div class="container"> 
      
      <!-- Footer Upside Links -->
        <!-- Categories -->
        <div class="col-md-2">
          <h4>Information</h4>
          <ul class="links-footer">
            <li><a href="#.">Our Blog</a></li>
            <li><a href="#."> About Our Shop</a></li>
            <li><a href="#."> Secure Shopping</a></li>
            <li><a href="#."> Delivery infomation</a></li>
            <li><a href="#."> Store Locations</a></li>
            <li><a href="#."> FAQs</a></li>
          </ul>
        </div>
      </div>
    </div>
  </footer>
  
  <!-- Rights -->
  <div class="rights">
    <div class="container">
      <div class="row">
        <div class="col-sm-6">
          <p>Copyright © 2017 <a href="#." class="ri-li"> UET Challenger </a></p>
        </div>
        <div class="col-sm-6 text-right"> <img src="./images/card-icon.png" alt=""> </div>
      </div>
    </div>
  </div>
  
  <!-- End Footer --> 
  
  <!-- GO TO TOP  --> 
  <a href="#" class="cd-top"><i class="fa fa-angle-up"></i></a> 
  <!-- GO TO TOP End --> 
</div>
